int_u32 u1 = optraits<int_u32>::max();
int_u16 u2 = optraits<int_u16>::max();
int_u8  u3 = optraits<int_u8>::max();
int_s32 s1 = optraits<int_s32>::max();
int_s16 s2 = optraits<int_s16>::max();
int_s8  s3 = optraits<int_s8>::max();
int_u32 u4 = optraits<int_u32>::min();
int_u16 u5 = optraits<int_u16>::min();
int_u8  u6 = optraits<int_u8>::min();
int_s32 s4 = optraits<int_s32>::min();
int_s16 s5 = optraits<int_s16>::min();
int_s8  s6 = optraits<int_s8>::min();
return u1 != optraits<int_u32>::sup()
    || u2 != optraits<int_u16>::sup()
    || u3 != optraits<int_u8>::sup()
    || s1 != optraits<int_s32>::sup()
    || s2 != optraits<int_s16>::sup()
    || s3 != optraits<int_s8>::sup()
    || u4 != optraits<int_u32>::inf()
    || u5 != optraits<int_u16>::inf()
    || u6 != optraits<int_u8>::inf()
    || s4 != optraits<int_s32>::inf()
    || s5 != optraits<int_s16>::inf()
    || s6 != optraits<int_s8>::inf();
