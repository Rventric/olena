//                                                                  -*- c++ -*-
#include "data.hh"
#include <oln/utils/md5.hh>

#include "check.hh"
#include <oln/basics2d.hh>
#include <oln/morpho/watershed.hh>
#include <oln/level/compare.hh>
#include <ntg/all.hh>

int main()
{
  oln::utils::key::value_type   data_key[16] = {0xc4, 0x69, 0x68, 0x78, 0xd6,
						0x80, 0x46, 0x20, 0x3f, 0x93,
						0x93, 0x46, 0x9c, 0x90, 0x8d,
						0x5b};

  oln::utils::key		   key(data_key);

  typedef oln::image2d<ntg::int_u8>	im_type;
  im_type	im1(oln::load(rdata("lena256.pgm")));

  if (oln::utils::md5(oln::morpho::watershed_seg<ntg::int_u16>(im1,
							       oln::neighb_c4())) == key)
    {
      std::cout << "OK" << std::endl;
      return true; // XFAIL Don't give the same result with icc and g++
    }
  else
    {
      std::cout << "FAIL" << std::endl;
      return true;
    }
  return 0;
 }
