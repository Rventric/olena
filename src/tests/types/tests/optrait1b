int_u32 u1 = oln::optraits<int_u32>::max();
int_u16 u2 = oln::optraits<int_u16>::max();
int_u8  u3 = oln::optraits<int_u8>::max();
int_s32 s1 = oln::optraits<int_s32>::max();
int_s16 s2 = oln::optraits<int_s16>::max();
int_s8  s3 = oln::optraits<int_s8>::max();
int_u32 u4 = oln::optraits<int_u32>::min();
int_u16 u5 = oln::optraits<int_u16>::min();
int_u8  u6 = oln::optraits<int_u8>::min();
int_s32 s4 = oln::optraits<int_s32>::min();
int_s16 s5 = oln::optraits<int_s16>::min();
int_s8  s6 = oln::optraits<int_s8>::min();
return u1 != oln::optraits<int_u32>::sup()
    || u2 != oln::optraits<int_u16>::sup()
    || u3 != oln::optraits<int_u8>::sup()
    || s1 != oln::optraits<int_s32>::sup()
    || s2 != oln::optraits<int_s16>::sup()
    || s3 != oln::optraits<int_s8>::sup()
    || u4 != oln::optraits<int_u32>::inf()
    || u5 != oln::optraits<int_u16>::inf()
    || u6 != oln::optraits<int_u8>::inf()
    || s4 != oln::optraits<int_s32>::inf()
    || s5 != oln::optraits<int_s16>::inf()
    || s6 != oln::optraits<int_s8>::inf();
