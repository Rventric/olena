# Conversion rules.

.png.ppm:
	pngtopnm $< > $@

.ppm.pgm:
	ppmtopgm $< > $@

.pgm.pbm:
	pgmtopbm $< > $@

.ppm.pppm:
	pnmtoplainpnm $< > $@

.pgm.ppgm:
	pnmtoplainpnm $< > $@

.pbm.ppbm:
	pnmtoplainpnm $< > $@
mm.gz:
	gzip -c -9 $< > $@

data/lena128.pgm: data/lena.pgm
	convert -scale 25% data/lena.pgm $@
