CC = @@CXX@@
CXX = @@CXX@@
CXXFLAGS = @@CFLAGS@@ -I@@SRCDIR@@ -Ifixtures
LDFLAGS = @@LIBRUBYARG@@

simple: test_function_loader.o test_function_loader
	./test_function_loader

containers: test_containers.o test_containers
	./test_containers

damien: test_damien.o test_damien
	./test_damien

# -I${PATH_TO_OLN} 
olena: test_olena.o test_olena
	./test_olena

vaucanson:
	cd vaucanson && $(MAKE) check

check: simple containers damien

clean:
	rm -f test_function_loader test_damien test_olena test_containers
	rm -f *.o

repository_clean: clean
	rm -rf repository
