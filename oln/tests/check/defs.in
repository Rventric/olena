# -*- shell-script -*-

# User can set VERBOSE to see all output.
if test "x$VERBOSE" = x; then
   exec > /dev/null 2>&1
fi

SHELL="@SHELL@"
EXEEXT="@EXEEXT@"
CXX="@CXX@"
CXXFLAGS="@CXXFLAGS@ @OLN_CXXFLAGS@ @CXXFLAGS_OPTIMIZE@ @CXXFLAGS_STRICT_ERRORS@"
CPPFLAGS="@CPPFLAGS@ @OLN_CPPFLAGS@ @DEFS@ -I${TOP_BUILDDIR:?} -I${PATH_TO_OLN_SRCDIR:?} -I${PATH_TO_OLN_BUILDDIR:?}"
LDFLAGS="@LDFLAGS@"

# Turn on shell traces when VERBOSE=x.
if test "x$VERBOSE" = xx; then
  set -x
else
  :
fi

