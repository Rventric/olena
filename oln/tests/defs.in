# -*- shell-script -*-

# User can set VERBOSE to see all output.
test -z "$VERBOSE" && {
   exec > /dev/null 2>&1
}

CXX=@CXX@
abs_top_builddir="@abs_top_builddir@"
abs_top_srcdir="@abs_top_srcdir@"
OLN_INCLUDE_DIR="@OLN_INCLUDE_DIR@"
OLN_IMG_DIR="@OLN_IMG_DIR@"
OLN_IMG_AUX_DIR="@OLN_IMG_AUX_DIR@"

CXXFLAGS="@CXXFLAGS@ @OLN_CXXFLAGS@ @CXXFLAGS_STRICT_ERRORS@"
CPPFLAGS="@CPPFLAGS@ @OLN_CPPFLAGS@ @DEFS@ -I$abs_top_builddir"
LDFLAGS="@LDFLAGS@"

# Turn on shell traces when VERBOSE=x.
if test "x$VERBOSE" = xx; then
  set -x
else
  :
fi

