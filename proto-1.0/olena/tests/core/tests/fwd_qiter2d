							// -*- C++ -*-
#include <oln/core/2d/window2d.hh>

#include <cassert>

using namespace oln;

template <typename E>
bool foo(const E& w)
{
  oln_wn_type_of(E, fwd_iter) q(w);
  unsigned cpt = 0;
  point2d p;
  for_all_q_of_p(q, p)
    ++cpt;
  return not (cpt == w.card());
}

int main()
{
  window2d w = win_c8p();
  return foo(w);
}
